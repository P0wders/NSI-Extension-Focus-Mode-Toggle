{
  "manifest_version": 2,
  "name": "Raisintine Anti-Focus",
  "version": "1.0",
  "description": "Disable the Question Focus mode on raisintine.fr",
  "content_scripts": [
    {
      "matches": ["https://raisintine.fr/chocolatine/question.php*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
